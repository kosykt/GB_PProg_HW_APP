<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.GB_PProg" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/secondaryTextSelectHandle</item>
        <item name="colorPrimaryVariant">@color/secondaryBackgroundColor</item>
        <item name="colorOnPrimary">@color/test_red</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/test_green</item>
        <item name="colorSecondaryVariant">@color/test_red</item>
        <item name="colorOnSecondary">@color/test_red</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:navigationBarColor">@color/secondaryLightColor</item>
        <item name="android:windowBackground">@color/secondaryBackgroundColor</item>
        <item name="android:textColorHighlight">@color/secondaryTextColorHighlight</item>
        <item name="android:textCursorDrawable">@drawable/cursor</item>
        <item name="attrTranslatorView">@style/translator_view_style.dark_theme</item>
        <item name="attrTranslatorCvInput">@style/translator_cv_input_style.dark_theme</item>
        <item name="attrTranslatorTil">@style/translator_til_style.dark_theme</item>
        <item name="attrTranslatorTiet">@style/translator_tiet_style.dark_theme</item>
        <item name="attrTranslatorItemCv">@style/translator_item_cv_style.dark_theme</item>
        <item name="attrTranslatorItemTvTranslate">
            @style/translator_item_tv_translate_style.dark_theme
        </item>
        <item name="attrTranslatorItemTvNote">@style/translator_item_tv_note_style.dark_theme</item>
        <item name="attrActivityBottomNavView">@style/activity_bottom_nav_view_style.dark_theme
        </item>
    </style>
</resources>